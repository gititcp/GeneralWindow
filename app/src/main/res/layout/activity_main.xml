<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/y10">

        <!--Toast-->
        <TextView
            android:id="@+id/tv_toast"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:background="@drawable/dialog_drawable_normal_radius_ture_stroke_true_cancel_selector"
            android:gravity="center"
            android:text="Toast" />

        <!--Dialog-->
        <TextView
            android:id="@+id/tv_dialog"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:layout_marginTop="@dimen/y10"
            android:background="@drawable/dialog_drawable_normal_radius_ture_stroke_true_cancel_selector"
            android:gravity="center"
            android:text="Dialog" />

        <!--LoadingDialog-->
        <TextView
            android:id="@+id/tv_loading_dialog"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:layout_marginTop="@dimen/y10"
            android:background="@drawable/dialog_drawable_normal_radius_ture_stroke_true_cancel_selector"
            android:gravity="center"
            android:text="LoadingDialog" />

        <!--UploadHeadPortraitDilog-->
        <TextView
            android:id="@+id/tv_upload_dialog"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:layout_marginTop="@dimen/y10"
            android:background="@drawable/dialog_drawable_normal_radius_ture_stroke_true_cancel_selector"
            android:gravity="center"
            android:text="UploadHeadPortraitDilog" />

        <!--PopupWindowCheckListOne-->
        <TextView
            android:id="@+id/tv_popupWindowCheckListOne"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:layout_marginTop="@dimen/y10"
            android:background="@drawable/dialog_drawable_normal_radius_ture_stroke_true_cancel_selector"
            android:gravity="center"
            android:text="PopupWindowCheckListOne" />

        <!--PopupWindowCheckListTwo-->
        <TextView
            android:id="@+id/tv_popupWindowCheckListTwo"
            android:layout_width="match_parent"
            android:layout_height="@dimen/y40"
            android:layout_marginTop="@dimen/y10"
            android:background="@drawable/dialog_drawable_normal_radius_ture_stroke_true_cancel_selector"
            android:gravity="center"
            android:text="PopupWindowCheckListTwo" />

    </LinearLayout>

</ScrollView>
